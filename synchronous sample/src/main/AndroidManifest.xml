<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.sdk.payment.eway.com.synchronoussample">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="rapidandroidsdk.sdk.payment.eway.com.synchronoussample.SampleMainActivity"
            android:label="@string/app_name"
            android:exported="true"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="rapidandroidsdk.sdk.payment.eway.com.synchronoussample.EncryptionActivity"
            android:label="@string/title_activity_encryption"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="rapidandroidsdk.sdk.payment.eway.com.synchronoussample.CodeLookUpActivity"
            android:label="@string/title_activity_code_look_up"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
